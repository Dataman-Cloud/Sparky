{
  "appName": "advancetemplate",
  "cmd": null,
  "cpus": 0.1,
  "mem": 32,
  "mode": "replicates",
  "runAs": "用户名/组名称",
  "disk": 0,
  "instances": 1,
  "constraints": "equal vcluster 集群名称",
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "nginx:latest",
      "network": "bridge",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "TCP",
          "name": "web",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "healthCheck": {
    "protocol": "HTTP",
    "path": "/",
    "delaySeconds": 2,
    "gracePeriodSeconds": 5,
    "intervalSeconds": 30,
    "portName": "web",
    "timeoutSeconds": 3,
    "consecutiveFailures": 5,
    "ignoreHttp1xx": false
  }
}
